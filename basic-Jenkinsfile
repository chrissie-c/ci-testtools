def runStuff(String nodeName, Map info)
{
    println("runstuff: ${nodeName}")
    node("${nodeName}") {
	stage("do it on ${nodeName}") {
	    println("${env.NODE_NAME}")
	}
    }
    println("end")
}

def buildRun(Map info)
{
    def runmap = [:]
    def allnodes = getAllNodes()
    for (i in allnodes) {
	runmap["_${i}"] = {
	    runStuff(i, info)
	}
    }
    return runmap
}

def info = ['test':'value']

pipeline {
    agent { label 'built-in' }

    stages {
	// This is the main stage that covers everything
	stage('go') {
	    steps {
		script {
		    def map = buildRun(info)
		    println(map)
		    parallel map
		}
	    }
	}
    }
}
