
pipeline {
    agent { label 'built-in' }

    stages {
	// This is the main stage that covers everything
	stage('getnodes') {
	    steps {
		script {
		    allnodes = getAllNodes()
		    echo allnodes
		    node('built-in') {
			steps {
			    println ("${env.NODE_NAME}")
			    sh 'env|sort'
			}
		    }
		}
	    }
	}
    }
}

