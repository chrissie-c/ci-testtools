
pipeline {
    agent {
        label "built-in"
    }
        
    options {
        timestamps()
    }
        
    stages {
        stage('agents-tasks') {
            
            steps {
                script {
		    rh_env = collectenv('rh')
		    deb_env = collectenv('debian')

                    parallel rh_env + deb_env
                }
            }
        }
    }
}
