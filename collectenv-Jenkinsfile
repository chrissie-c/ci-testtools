    agent {
        label "fedora"
    }

//    options {
	// We do our own checkouts
//	skipDefaultCheckout(true)
//    }

    stages {
        stage('Do things') {

            steps {
                script {
		    sh "sh autogen.sh && ./configure"
		    sh "cat libqb.spec.in | sed 's/@version@/2.0.test/' > libqb.spec.2 && mv libqb.spec.2 libqb.spec.in"
		    sh "make rpm"
		    sh "jfrog rt upload *.rpm"
                }
            }
        }
    }
}
